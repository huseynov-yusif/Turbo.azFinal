
<!DOCTYPE html>
<html >
<head>
    <meta charset="utf-8" />
    <title>
        TurboAdmin
    </title>
    <link href="~/assets/favicons/Logo.png" rel="icon" sizes="192x192" type="image/png" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link href="~/assets/Admin/css/style.css" rel="stylesheet" />
    <link href="~/assets/Admin/css/bootstrap/bootstrap.min.css" rel="stylesheet" />
    @RenderSection("addcss", required: false);
</head>
<body class="crm_body_bg">


    <nav class="sidebar dark_sidebar">
        <div class="logo d-flex justify-content-between">
            <a a class="large_logo" href="index.html"><img src="~/assets/turbo-logo-az-24c9ce8421aec1428f697bce5033b652750bbc9262031438fea7f35fc37f8f84.png" alt=""></a>
            <div class="sidebar_close_icon d-lg-none">
                <i class="ti-close"></i>
            </div>
        </div>
        <ul id="sidebar_menu">
            <li class="">
                <a asp-area="admin" asp-controller="dashboard" asp-action="Logout">

                    <div class="nav_title">
                        <span>Logout</span>
                    </div>
                </a>
            </li>

            <li class="">
